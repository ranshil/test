node {
   echo 'Hello World'


stage('ero1') {
  echo 'Hello 111 World'}

sleep time: 200, unit: 'NANOSECONDS'

stage('ero2') {
  echo 'Hello 222 World'

	timestamps {
			try {
				sh "./gradlew run"
			} catch (err) {
				step([$class: 'JUnitResultArchiver', testResults: 'build/test-results/TEST-*.xml'])
				throw err
			}
		}



	}
}
